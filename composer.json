{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "laravel/framework": "^6.20.26",
    "symfony/console": "4.3.4",
    "symfony/debug": "~4.3.4",
    "symfony/event-dispatcher": "~4.3",
    "symfony/finder": "~4.3.4",
    "symfony/http-foundation": "~4.3.4",
    "symfony/http-kernel": "~4.3.4",
    "symfony/process": "~4.3.4",
    "symfony/routing": "~4.3.4",
    "symfony/translation": "~4.0",
    "symfony/var-dumper": "~4.3.4",

    "laravelcollective/html": "~6.0",
    "gghughunishvili/entrust": "~4.0.0",
    "rap2hpoutre/laravel-log-viewer": "2.0.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "pda/pheanstalk": "~4.0",
    "guzzlehttp/guzzle": "~6.3.1",
    "phpoffice/phpword": "dev-master",
    "maatwebsite/excel": "~3.1.33",
    "ext-json": "*",
    "laravel/helpers": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.8|^9.3.3",
    "phpspec/phpspec": "~6.1.0",
    "mockery/mockery": "^1.0",
    "barryvdh/laravel-ide-helper": "~2.8",
    "barryvdh/laravel-debugbar": "~3.6.4",
    "symfony/dom-crawler": "~4.3.4",
    "symfony/css-selector": "~4.3.4"

  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Nrgi/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
